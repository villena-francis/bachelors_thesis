	keywords = {Evolutionary biology, Virology},
	pages = {106210},
	file = {ScienceDirect Snapshot:/home/villena/Zotero/storage/G3MBGC3L/S2589004223002870.html:text/html;Takada et al. - 2023 - Genomic diversity of SARS-CoV-2 can be accelerated.pdf:/home/villena/Zotero/storage/TL879PRL/Takada et al. - 2023 - Genomic diversity of SARS-CoV-2 can be accelerated.pdf:application/pdf;Texto completo:/home/villena/Zotero/storage/FFIZMN42/Takada et al. - 2023 - Genomic diversity of SARS-CoV-2 can be accelerated.pdf:application/pdf},
